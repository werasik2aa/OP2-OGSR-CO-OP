function has_timer(name)
	--get_console():execute("load ~~~ TIMERS has_timer: "..name)
	local timers = load_table("timers")
	--dump_table(timers)
	local result = false
	for i = 1, #timers do
		if timers[i][1] == name then
			result = true
			break
		end
	end

	return result
--[[  for a=1,100,1 do
    tmp=load_variable("xt"..a,nil)
    if tmp and tmp==name then
      return true
    end
  end
  return false]]
end

function remove_timer(name)
	--get_console():execute("load ~~~ TIMERS has_timer: "..name)
	local timers = load_table("timers")
	--dump_table(timers)
	for i = #timers, 1, -1 do
		if timers[i][1] == name then
			table.remove(timers, i)
		end
	end

	-- сортируем для оптимизации дальнейшего поиска по таймерам
	-- таймеры с меньшим временем будут в конце таблицы
	table_sort(timers, timersSort)

	save_table("timers", timers)
end

